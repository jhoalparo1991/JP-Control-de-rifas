                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select 
	count(distinct a.id) contar,
	c.id,
	c.nombre_completo
from tbl_boletas a
inner join tbl_abonos_boleta b on a.id=b.boleta_id
inner join tbl_usuarios c on b.vendedor_id=c.id
where a.vendida=1
group by c.id,
	c.nombre_completo</CommandText>
